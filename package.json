{
  "name": "paper-emr",
  "version": "1.0.0",
  "description": "PageMD - Simple Paper-Style EMR MVP for Clinic",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install",
    "setup": "bash setup.sh",
    "init": "bash setup.sh"
  },
  "keywords": [
    "emr",
    "ehr",
    "medical",
    "healthcare"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "bcryptjs": "^3.0.3"
  }
}
